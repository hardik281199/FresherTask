<%- include("./partials/header") %>

  <div class="container">
    <h2><%= title %></h2>
    
    <form action="/save" method="post">
      <div class="form-group">
        <label for="emp_name">Name:</label>
        <input type="text" class="form-control" id="emp_name" placeholder="Enter Name" name="name" >
      </div>
      <div class="form-group">
        <label for="emp_salary">Email:</label>
        <input type="number" class="form-control" id="emp_salary" placeholder="Enter Salary" name="salary" >
      </div>
      <div class="form-group">
        <label for="emp_city">Phone No:</label>
        <input type="text" class="form-control" id="emp_city" placeholder="Enter City" name="city" >
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </div>
   
<%- include("./partials/footer") %>